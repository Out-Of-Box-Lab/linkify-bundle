{
    "name": "outofboxlab/linkify-bundle",
    "type": "symfony-bundle",
    "description": "Converts URLs and email addresses in text into HTML links",
    "keywords": [
        "url",
        "email address",
        "convert",
        "link",
        "bundle"
    ],
    "homepage": "https://github.com/Out-Of-Box-Lab/linkify-bundle",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Out-Of-Box-Lab/linkify-bundle/issues"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/framework-bundle": "^5.0 || ^6.0",
        "symfony/templating": "^5.0 || ^6.0",
        "misd/linkify": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0",
        "symfony/twig-bundle": "^5.0 || ^6.0",
        "symfony/yaml": "^5.0 || ^6.0"
    },
    "conflict": {
        "phpunit/phpunit-mock-objects": "<2.2",
        "twig/twig": "<1.23"
    },
    "suggest": {
        "symfony/twig-bundle": "to use the Twig linkify filter"
    },
    "autoload": {
        "psr-4": {
            "Misd\\LinkifyBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.2-dev"
        }
    }
}
